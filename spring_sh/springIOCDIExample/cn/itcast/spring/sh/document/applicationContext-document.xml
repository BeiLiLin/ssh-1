<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="documentManager" class="cn.itcast.spring.sh.document.DocumentManager">
		<constructor-arg index="0" ref="excelDocument"></constructor-arg>
	</bean>
	
	<bean id="wordDocument" class="cn.itcast.spring.sh.document.WordDocument"></bean>
	
	<bean id="pdfDocument" class="cn.itcast.spring.sh.document.PDFDocument"></bean>
	
	<bean id="excelDocument" class="cn.itcast.spring.sh.document.ExcelDocument"></bean>
</beans>
